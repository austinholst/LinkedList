#ifndef HEADERGUARD
#define HEADERGUARD

using namespace std;

class Node {
 public:
  Node(Student* student); //Constructor
  ~Node(); //Deconstructor
  Node* getNext(); //get the next Node*
  Student* getStudent(); //get student*
  void setNext(Node* newNext); //set the next pointer to the corresponding node pointer
 private:
  Node* next;
  Student* student;
}
#endif
